  "description": "rapper, use http api as a function",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "build": "rimraf dist && ./node_modules/typescript/bin/tsc -p tsconfig.json && rimraf runtime && node ./script/move-runtime",
    "normal": "npm run build && node ./dist/build/normal",
    "redux": "npm run build && node ./dist/build/redux",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "yarn build && yarn changelog && git add CHANGELOG.md"
  },
  "bin": {
    "service-gen": "./dist/models.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thx/rapper.git"
  },
  "keywords": [
    "api",
    "rap",
    "rapper"
  ],
  "author": "chibing.fy",
  "license": "ISC",
  "files": [
    "dist",
    "runtime"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "commander": "^4.0.1",
    "inquirer": "^7.0.4",
    "json-schema-to-typescript": "^7.1.0",
    "json5": "^2.1.0",
    "jsonschema": "^1.2.6",
    "latest-version": "^5.1.0",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "ora": "^4.0.3",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@types/json5": "0.0.30",
    "@types/lodash": "^4.14.144",
    "@types/react": "^16.9.2",
